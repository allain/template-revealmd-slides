{
  "name": "template-revealmd-slides",
  "version": "1.0.0",
  "private": true,
  "description": "A template for writing revealmd slides.",
  "main": "index.js",
  "scripts": {
    "build": "reveal-md --static dist/ --preprocessor ./preprocess.js --css custom.css slides.md",
    "dev": "reveal-md --watch --preprocessor ./preprocess.js --css custom.css slides.md"
  },
  "keywords": [
    "Revealmd",
    "slides",
    "reveal.js"
  ],
  "author": "Allain Lalonde <allain.lalonde@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "reveal-md": "^5.4.0"
  }
}
